rs.filter(el => el._id !== id)\n  })\n}\n\n\n\n\n\n usersList() {\n  let listedusers = (this.state.Data.length >0)? this.state.data :this.state.users;\n\n  return listedusers.map(currentUser => {\n    return <Profileuser user= { currentUser } deleteUser = { this.deleteUser} key = { currentUser._id }/>; \n  })\n} \n\n\n\n\n\n\n\n\n    // it addes the values of the input fileds in the states\n    handleChangeImage(e) {\n          if (e.target.files[0]) {\n              this.setState({\n              image: e.target.files[0]\n              })\n          }\n        \n      }\n     // it handles the upload of the picture in the firbase\n      handleUpload () {\n        var uploadTask = storage.ref(`images/${this.state.image.name}`).put(this.state.image);\n          uploadTask.on(\n            \"state_changed\",\n            snapshot => {\n              var progress = Math.round(\n                (snapshot.bytesTransferred / snapshot.totalBytes) * 100\n              );\n              this.setState({\n                progress:progress})\n              },\n              error => {\n              console.log(error);\n             },\n              () => {\n                storage\n                .ref(\"images\")\n                .child(this.state.image.name)\n                .getDownloadURL()\n                .then(url => {\n                  this.setState({\n                    url : url\n                })\n                });\n                }\n                );\n             }\n  \n             render() {\n                return (\n                  <div>\n                  <br />\n                  <div className = \"container text-center border border-light p-9\">\n                  <h2>Hello User</h2>\n                   <p>  user information </p>\n                <table className = \"table\">\n                <thead className = \"thead\">\n                    <tr>\n                        <th>User Name</th>\n                        <th>Password</th>\n                        <th>Phone</th>\n                        <th>Address</th>\n                        <th>Image </th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {this.usersList()}\n                </tbody>\n             \n\n               </table>\n\n               </div>\n                            <div className = \"col\">\n                            <label>Image</label>\n                           <div  id='image' > <img src={this.state.url || \"http://via.placeholder.com/50 50\"} \n                            alt=\"firebase\"  /></div> \n                           <input  type=\"file\" onChange={this.handleChangeImage.bind(this)} className=\"btn btn-deep-orange darken-4\" />\n                           <button  onClick={this.handleUpload.bind(this)} className=\"btn btn-deep-orange darken-4\">Upload</button>\n                           </div>\n                            <br />\n                           \n                    <Footer />\n                  </div>\n                    \n                )\n              }\n            }\n            \n            export default withRouter(Personalprofile)\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n","/Users/rbk07/Desktop/legacy-project-/src/components/edituser.js",[],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","severity":1,"message":"91","line":7,"column":8,"nodeType":"92","messageId":"93","endLine":7,"endColumn":15},{"ruleId":"94","severity":1,"message":"95","line":4,"column":26,"nodeType":"96","messageId":"97","endLine":4,"endColumn":41,"fix":"98"},{"ruleId":"99","severity":1,"message":"100","line":69,"column":72,"nodeType":"101","endLine":69,"endColumn":96},{"ruleId":"99","severity":1,"message":"100","line":76,"column":93,"nodeType":"101","endLine":76,"endColumn":121},{"ruleId":"102","severity":1,"message":"103","line":34,"column":46,"nodeType":"101","endLine":34,"endColumn":61},{"ruleId":"102","severity":1,"message":"103","line":41,