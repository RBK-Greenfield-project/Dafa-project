   <td>\r\n      <Link\r\n        to={\"/edit/\" + props.item._id}\r\n        className=\"btn btn-deep-orange darken-4\"\r\n      >\r\n        Edit\r\n      </Link>\r\n      <button\r\n        type=\"button\"\r\n        className=\"btn btn-deep-orange darken-4\"\r\n        onClick={() => {\r\n          props.deleteItem(props.item._id);\r\n        }}\r\n      >\r\n        Delete\r\n      </button>\r\n    </td>\r\n  </tr>\r\n);\r\n\r\nclass ItemsList extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n\r\n    this.deleteItem = this.deleteItem.bind(this);\r\n\r\n    this.state = {\r\n      items: [],\r\n      filteredItems: [],\r\n      SearchString: \"\",\r\n    };\r\n  }\r\n\r\n  componentDidMount() {\r\n    axios\r\n      .get(\"http://localhost:3000/addItems/\")\r\n      .then((res) => {\r\n        console.log(res.data.length);\r\n        this.setState({ items: res.data });\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n      });\r\n  }\r\n\r\n  deleteItem(id) {\r\n    axios\r\n      .delete(\"http://localhost:3000/addItems/\" + id)\r\n      .then((res) => console.log(res.data));\r\n    this.setState({\r\n      items: this.state.items.filter((el) => el._id !== id),\r\n    });\r\n  }\r\n\r\n  itemsList() {\r\n    let listedItems =\r\n      this.state.filteredItems.length > 0\r\n        ? this.state.filteredItems\r\n        : this.state.items;\r\n\r\n    return listedItems.map((currentItem) => {\r\n      return (\r\n        <ClothesItem\r\n          item={currentItem}\r\n          deleteItem={this.deleteItem}\r\n          key={currentItem._id}\r\n        />\r\n      );\r\n    });\r\n  }\r\n\r\n  onSearch = (e) => {\r\n    let { items } = this.state;\r\n    let string = e.target.value;\r\n    if (string.length > 0) {\r\n      let filteredItems = items.filter((item) =>\r\n        item.itemName.includes(string)\r\n      );\r\n      this.setState({ SearchString: string, filteredItems: filteredItems });\r\n    } else this.setState({ SearchString: string, filteredItems: [] });\r\n  };\r\n\r\n  render() {\r\n    return (\r\n      <div>\r\n        <Navbar_Login />\r\n        <br />\r\n        <div className=\"container text-center border border-light p-9\">\r\n          <h2>Clothing</h2>\r\n          <input\r\n            name=\"search\"\r\n            className=\"form-control\"\r\n            onChange={(e) => this.onSearch(e)}\r\n            value={this.state.SearchString}\r\n            placeholder=\"Search for item Name\"\r\n          />\r\n          <table className=\"table\">\r\n            <thead className=\"thead\">\r\n              <tr>\r\n                <th>Item Name</th>\r\n                <th>Category</th>\r\n                <th>Type</th>\r\n                <th>Description</th>\r\n                <th>Image</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>{this.itemsList()}</tbody>\r\n          </table>\r\n        </div>\r\n        <Footer />\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default withRouter(ItemsList);\r\n","/Users/rbk24/Desktop/legacy-project-/src/components/AddItems.js",["87"],"/Users/rbk24/Desktop/legacy-project-/src/components/Navbar.js",["88"],"import React, { Component } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport \"font-awesome/css/font-awesome.min.css\";\r\nimport Counter from \"./Counter\";\r\n\r\nexport default class Navbar extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n    this.state = {\r\n      counter: \"\",\r\n    };\r\n  }\r\n\r\n  render() {\r\n    return (\r\n      <div>\r\n        <nav\r\n          style={{ color: \"orange\" }}\r\n          className=\"navbar navbar-dark bg-dark navbar-expand-lg\"\r\n        >\r\n          <Link to=\"/Homepage\" className=\"navbar-brand\">\r\n            DAFAH\r\n          </Link>\r\n\r\n          <div className=\"collpase navbar-collapse\">\r\n            <ul className=\"navbar-nav mr-auto\">\r\n              <li className=\"navbar-item\">\r\n                <Link to=\"/ItemsList\" className=\"nav-link\">\r\n                  Clothes\r\n                </Link>\r\n              </li>\r\n              <li className=\"navbar-item\">\r\n                <Link to=\"/login\" className=\"nav-link\">\r\n                  Login\r\n                </Link>\r\n              </li>\r\n            </ul>\r\n\r\n            <a>\r\n              <i class=\"fa fa-heart\" aria-hidden=\"true\">\r\n                <Counter />\r\n              </i>\r\n            </a>\r\n          </div>\r\n        </nav>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n","/Users/rbk24/Desktop/legacy-project-/src/components/edit.js",[],"/Users/rbk24/Desktop/legacy-project-/src/components/Signup.js",[],"/Users/rbk24/Desktop/legacy-project-/src/components/Login.js",["89","90"],"import React, { Component } from \"react\";\r\nimport axios from \"axios\";\r\nimport Footer from \"./Footer\";\r\n// import Navbar_Login from \"./Navbar_Login\";\r\n\r\nexport default class Login extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n    this.onChangeUsername = this.onChangeUsername.bind(this);\r\n    this.onChangePassword = this.onChangePassword.bind(this);\r\n\r\n    this.onSubmit = this.onSubmit.bind(this);\r\n    this.state = {\r\n      username: \"\",\r\n      password: \"\",\r\n    };\r\n  }\r\n\r\n  onChangeUsername(e) {\r\n    this.setState({\r\n      username: e.target.value,\r\n    });\r\n  }\r\n  onChangePassword(e) {\r\n    this.setState({\r\n      password: e.target.value,\r\n    });\r\n  }\r\n\r\n  onSubmit(e) {\r\n    e.preventDefault();\r\n    //where we set the state and send it in the post request\r\n    const user = {\r\n      username: this.state.username,\r\n      password: this.state.password,\r\n    };\r\n\r\n    axios\r\n      .post(\"http://localhost:3000/addUser/login\", user)\r\n      .then((response) => {\r\n        // console.log (response)\r\n        window.localStorage.setItem(\"token\", response.data);\r\n\r\n        window.location = \"/Homepage2\";\r\n      })\r\n      .catch((err) => alert(\"username or password is incorrect\"));\r\n  }\r\n\r\n  render() {\r\n    return (\r\n      <div>\r\n        {/* <Navbar_Login /> */}\r\n        <br />\r\n        <div className=\"container\">\r\n          <form\r\n            className=\"text-center border border-light p-9\"\r\n            onSubmit={this.onSubmit}\r\n          >\r\n            <h3 className=\"mb-3\">Login</h3>\r\n            <br />\r\n            <div className=\"col\">\r\n              <label> User Name </label>\r\n              <br></br>\r\n              <input\r\n                required=\"true\"\r\n                className=\"col\"\r\n                type=\"text\"\r\n                className=\"form-control\"\r\n                value={this.setState.username}\r\n                onChange={this.onChangeUsername}\r\n                placeholder=\"Enter Your User Name\"\r\n              />\r\n              <br></br>\r\n            </div>\r\n            <br></br>\r\n            <div className=\"col\">\r\n              <label> Password </label>\r\n              <br></br>\r\n              <input\r\n                required=\"true\"\r\n                className=\"col\"\r\n                type=\"password\"\r\n                name=\"password\"\r\n                className=\"form-control col\"\r\n                value={this.setState.password}\r\n                onChange={this.onChangePassword}\r\n                placeholder=\"Enter Your Password\"\r\n              />\r\n            </div>\r\n            <br></br>\r\n            <input\r\n              type=\"submit\"\r\n              value=\"Log In\"\r\n              className=\"btn btn-deep-orange darken-4\"\r\n            />\r\n            <br></br>\r\n            <br></br>\r\n            <p>\r\n              Don't have an account? <a href=\"/addUser\"> Sign Up</a>\r\n            </p>\r\n          </form>\r\n        </div>\r\n        <Footer />\r\n      </div>\r\n    );\r\n  }\r\n}\r\n","/Users/rbk24/Desktop/legacy-project-/src/components/Carousel.js",[],"/Users/rbk24/Desktop/legacy-project-/src/components/Footer.js",["91","92","93","94"],"import React from \"react\";\r\n// import styled from \"styled-components\";\r\nimport { MDBCol, MDBContainer, MDBRow, MDBFooter } from \"mdbreact\";\r\nconst FooterPage = () => {\r\n  return (\r\n\r\n    <MDBFooter color=\"grey darken-4\" className=\"font-small pt-4 mt-4 w-auto p-3\">\r\n      <MDBContainer fluid className=\"text-center text-md-left\" style = {{padding:\"16px\", fontFamily: \"Century Gothic\"}}>\r\n        <MDBRow className = \"d-flex justify-content-around\">\r\n          <MDBCol md=\"3\" className=\"text-center\">\r\n            <h6 className=\"title\">About Us</h6> <br />\r\n            <p style = {{fontSize: \"12px\", letterSpacing: \"1.5px\", lineHeight: \"1.5\"}}>\r\n            DAFAH. A charity organization aimed towards creating a\r\n            direct connection between the donor and the organization\r\n            whereas anyone can open the website, create an account and simply\r\n            submit a form with the item that they want to donate. We will then\r\n            contact them to pick up the item to take it to its new home. \r\n            </p>\r\n          </MDBCol>\r\n          <MDBCol md=\"3\" className=\"text-center\">\r\n            <h6 className=\"title\">Contact Us</h6> <br />\r\n              <p style = {{fontSize: \"12px\", letterSpacing: \"1.5px\", lineHeight: \"1.5\"}}>ADDRESS <br />\r\n              Al- Sha'ab St. 29, Al-Jandaweel, Amman, Jordan <br />\r\n              PHONE # <br />\r\n              <a href=\"tel:+962 7 980 7680\">Call us at +962 7 980 7680</a> <br />\r\n              <a href=\"tel:+962 7 800 7680\">Call us at +962 7 800 7680</a> <br />\r\n              E-MAIL<br />\r\n              <a href = \"mailto:dafah.organization@dafah.com\">dafah.organization@dafah.com</a> </p>\r\n          </MDBCol>\r\n          <div></div>\r\n    